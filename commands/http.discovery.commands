echo "\e[92m--- Running dirsearch for target: _TARGET_ ---\e[0m"
sudo dirsearch -u _TARGET_ --plain-text-report="_OUTPUT_/_DOMAIN_.dirseach.txt" -e ""
echo "\e[92m--- Running dirb for target: _TARGET_ ---\e[0m"
dirb  _TARGET_ -o "_OUTPUT_/_DOMAIN_.dirb.txt" -X ""
echo "\e[92m--- Running opendoor for target: _TARGET_ ---\e[0m"
opendoor --host _TARGET_ --reports txt --reports-dir "_OUTPUT_/_DOMAIN_.opendoor"
echo "\e[92m--- Running gobuster for target: _TARGET_ ---\e[0m"
gobuster dir --wildcard -v -o "_OUTPUT_/_DOMAIN_.gobuster.txt" -w "_APP_PATH_/brute-force/common_discovery.small.txt" -u "_TARGET_"